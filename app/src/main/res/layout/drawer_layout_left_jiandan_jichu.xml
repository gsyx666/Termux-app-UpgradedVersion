<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <main.java.com.termux.utils.CustomTextView
            android:layout_width="wrap_content"
            android:text="@string/源功能ghjghj110"
            android:textStyle="bold"
            android:layout_marginTop="2dp"
            android:textSize="12sp"
            android:layout_marginBottom="2dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="@color/color_FFFFFF"
            android:layout_height="wrap_content"/>



    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/jiandan_yuan_click"
            android:layout_height="120dp">

           <LinearLayout
               android:layout_width="wrap_content"
               android:orientation="vertical"
               android:gravity="center"
               android:layout_centerVertical="true"
               android:layout_centerHorizontal="true"
               android:layout_height="wrap_content">

               <ImageView
                   android:layout_width="40dp"
                   android:layout_height="40dp"
                   android:src="@drawable/beijingyuan" />


               <main.java.com.termux.utils.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="3dp"
                   android:layout_weight="1"
                   android:gravity="center"
                   android:text="@string/北京大学源222"
                   android:textColor="#fff"
                   android:textSize="13sp"
                   android:textStyle="bold" />

               <main.java.com.termux.utils.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="3dp"
                   android:layout_weight="1"
                   android:gravity="center"
                   android:text="@string/北京大学源2223"
                   android:textColor="#fff"
                   android:textSize="10sp"
                   android:textStyle="bold" />

           </LinearLayout>



            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/color_553E3E3E"
                app:cardCornerRadius="5dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@color/color_FFFFFF"
                    android:src="@drawable/jiandan_beij" />

            </androidx.cardview.widget.CardView>


        </RelativeLayout>



    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <main.java.com.termux.utils.CustomTextView
            android:layout_width="wrap_content"
            android:text="@string/切换功能123fddf"
            android:textStyle="bold"
            android:layout_marginTop="2dp"
            android:textSize="12sp"
            android:layout_marginBottom="2dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="@color/color_FFFFFF"
            android:layout_height="wrap_content"/>



    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/jiandan_qiehuan_click"
            android:layout_height="120dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/jiandan_qiehuan" />


                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/切换系统54545"
                    android:textColor="#fff"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/创建以及切换多个系统ppp"
                    android:textColor="#fff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

            </LinearLayout>



            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/color_553E3E3E"
                app:cardCornerRadius="5dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@color/color_FFFFFF"
                    android:src="@drawable/jiandan_qiehuan_xia" />

            </androidx.cardview.widget.CardView>


        </RelativeLayout>



    </androidx.cardview.widget.CardView>







    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <main.java.com.termux.utils.CustomTextView
            android:layout_width="wrap_content"
            android:text="@string/恢复功能555f"
            android:textStyle="bold"
            android:layout_marginTop="2dp"
            android:textSize="12sp"
            android:layout_marginBottom="2dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:textColor="@color/color_FFFFFF"
            android:layout_height="wrap_content"/>



    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/jiandan_jisu_click"
            android:layout_height="120dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/jiandan_huifu" />


                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/恢复包数据包恢复45"
                    android:textColor="#fff"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/恢复下载站54"
                    android:textColor="#fff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

            </LinearLayout>



            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/color_553E3E3E"
                app:cardCornerRadius="5dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@color/color_FFFFFF"
                    android:src="@drawable/jiandan_huifu_xia" />

            </androidx.cardview.widget.CardView>


        </RelativeLayout>



    </androidx.cardview.widget.CardView>





</LinearLayout>
